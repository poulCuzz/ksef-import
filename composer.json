{
    "name": "poulcuzz/ksef-export",
    "description": "Import faktur z KSeF (Krajowy System e-Faktur) - klient PHP",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "poulCuzz",
            "homepage": "https://github.com/poulCuzz"
        }
    ],
    "require": {
        "php": ">=8.0",
        "phpseclib/phpseclib": "^3.0",
        "symfony/http-client": "^7.4",
        "nyholm/psr7": "^1.8",
        "lyquidity/xml-signer": "^1.12"
    },
    "autoload": {
        "psr-4": {
            "KSeF\\": "src/"
        }
    },
    "scripts": {
        "start": "php -S localhost:8000"
    },
    "keywords": [
        "ksef",
        "ksef-api",
        "e-faktura",
        "faktury",
        "invoice",
        "php",
        "vat",
        "poland"
    ],
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
